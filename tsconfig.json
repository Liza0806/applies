{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "jsx": "react-jsx",  
  },
  "include": [
    "src/**/*.ts"
  ]
}